   "source": [
    "def is_prime(num): \n",
    "\n",
    "    if num < 2: \n",
    "        return False\n",
    "\n",
    "    for i in range(2,int(num**0.5)+1): \n",
    "        if num % i == 0: \n",
    "            return False\n",
    "\n",
    "    return True"
   ]
