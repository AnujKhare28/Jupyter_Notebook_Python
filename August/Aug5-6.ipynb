   "source": [
    "n = int(input())\n",
    "m = n\n",
    "pow = 0\n",
    "binNum = 0\n",
    "\n",
    "while n>0 :\n",
    "    rem = n % 2\n",
    "    binNum = binNum + rem * (10 ** pow)\n",
    "    pow += 1\n",
    "    n = n//2\n",
    "print(f\"Binary form of {m} is {binNum}\")"
   ]
  }
