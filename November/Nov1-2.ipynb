    "def middleNode(head):\n",
    "\n",
    "    slow = head\n",
    "    fast = head \n",
    "    while fast != None and fast.next != None:\n",
    "        slow = slow.next\n",
    "        fast = fast.next.next\n",
    "    return slow"
