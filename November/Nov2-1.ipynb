   "source": [
    "temp = head\n",
    "stack = []\n",
    "while temp is not None: \n",
    "    stack.append(temp.val)\n",
    "    temp = temp.next\n",
    "temp = head\n",
    "while temp is not None:\n",
    "    e = stack.pop()\n",
    "    temp.val = e\n",
    "    temp = temp.next\n",
    "return head"
   ]
  }
