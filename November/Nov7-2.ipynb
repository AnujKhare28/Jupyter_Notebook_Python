   "source": [
    "def hasCycle(self, head):\n",
    "   \n",
    "    slow = head\n",
    "    fast = head\n",
    "\n",
    "\n",
    "    while fast is not None and fast.next is not None:\n",
    "\n",
    "        slow = slow.next\n",
    "        fast = fast.next.next\n",
    "        if slow == fast: \n",
    "            return True\n",
    "    return False\n"
   ]
