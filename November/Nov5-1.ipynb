   "source": [
    "import math\n",
    "\n",
    "class Circle: \n",
    "    def __init__(self, r): \n",
    "        self.r = r\n",
    "\n",
    "    def area(self): \n",
    "        return math.pi * self.r * self.r\n",
    "\n",
    "    def perimeter(self): \n",
    "        return 2 * math.pi * self.r\n",
    "\n",
    "c1 = Circle(7)\n",
    "print(c1.area())\n",
    "print(c1.perimeter())"
   ]
