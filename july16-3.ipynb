   "source": [
    "### Check the item present in a tuple"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "f4b5f659-4a19-466a-8aeb-8220db8c92b8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "True\n"
     ]
    }
   ],
   "source": [
    "mytup = ('Ram','Shyam','Krishna','Vishnu','Narayan')\n",
    "name = 'Krishna'\n",
    "\n",
    "if name in mytup:\n",
    "    print(\"True\")\n",
    "else:\n",
    "    print('False')"
   ]
  },
  {
