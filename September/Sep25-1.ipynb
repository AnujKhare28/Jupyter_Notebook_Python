   "source": [
    "def largest(arr):\n",
    "    largest = float('-inf')\n",
    "    \n",
    "    n = len(arr)\n",
    "\n",
    "    for i in range(0, n):\n",
    "        if arr[i] > largest:\n",
    "            largest = arr[i]\n",
    "    return largest"
   ]
