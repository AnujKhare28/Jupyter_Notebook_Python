   "source": [
    "def subArr(arr): \n",
    "\n",
    "    n = len(arr)\n",
    "\n",
    "    for i in range(n): \n",
    "\n",
    "        for j in range(i, n):\n",
    "\n",
    "            for k in range(i, (j+1)): \n",
    "                \n",
    "                print(arr[k], end=\" \")\n",
    "                \n",
    "            print()"
   ]
