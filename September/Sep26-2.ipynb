   "source": [
    "def isSorted(arr):\n",
    "    n = len(arr)\n",
    "\n",
    "    for i in range(0,(n-1)):\n",
    "        if arr[i]>arr[i+1]:\n",
    "            return False\n",
    "    return True"
   ]
