    "arr = [1,2,3,4,5,6]\n",
    "rotateBy = 2\n",
    "n = len(arr)\n",
    "for i in range(rotateBy): \n",
    "    last = arr[n-1]\n",
    "    for j in range((n-1), 0,-1):\n",
    "        arr[j] = arr[j-1]\n",
    "    arr[0] = last"
