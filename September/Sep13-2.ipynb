   "source": [
    "arr = [1,4,5,3,2]\n",
    "n = len(arr)\n",
    "\n",
    "result = 0\n",
    "\n",
    "for i in range(n):\n",
    "\n",
    "    temp = 0\n",
    "\n",
    "    for j in range(i, n):\n",
    "\n",
    "        temp += arr[j]\n",
    "\n",
    "        result += temp\n",
    "\n",
    "print(result)"
   ]
