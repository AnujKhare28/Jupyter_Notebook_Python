    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "fact = 1\n",
    "\n",
    "if num < 0 :\n",
    "    print (\"Factorial of negative no. does not exist\")\n",
    "if num == 0 :\n",
    "    print (\"Factorial of Zero is 1\")\n",
    "if num > 0 :\n",
    "    for i in range (1, num + 1):\n",
    "        fact = fact * i\n",
    "print (f\"The Factorail of the {num} is {fact}\")"
