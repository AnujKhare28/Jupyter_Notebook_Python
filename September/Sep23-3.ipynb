    "def str_prefix(string):\n",
    "    if len(string) < 2: \n",
    "        return n*string\n",
    "    else: \n",
    "        return n*string[:2]\n",
    "string = input(\"Enter a string:\")\n",
    "n = int(input(\"Enter copies of string:\"))\n",
    "str_prefix(string)"
   ]
