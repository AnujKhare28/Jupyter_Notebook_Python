   "source": [
    "sharePrice = [100, 180, 260, 310, 40, 535, 695]\n",
    "\n",
    "n = len(sharePrice)\n",
    "\n",
    "maxProfit = 0\n",
    "minPrice = float('inf')\n",
    "\n",
    "for i in range(0,n): \n",
    "    minPrice = min(minPrice, sharePrice[i])\n",
    "    maxProfit = max(maxProfit,(sharePrice[i]-minPrice))\n",
    "\n",
    "print(maxProfit)"
   ]
