   "source": [
    "n = len(prices)\n",
    "max_profit = 0\n",
    "for i in range(0,n):\n",
    "\n",
    "    for j in range(i+1,n):\n",
    "        if prices[j] > prices[i]:\n",
    "            profit = prices[j] - prices[i]\n",
    "            max_profit = max(max_profit,profit)\n",
    "print(max_profit)"
   ]
