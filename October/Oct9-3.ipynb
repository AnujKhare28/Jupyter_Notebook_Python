   "source": [
    "n = len(nums)\n",
    "max_count = 0\n",
    "\n",
    "for i in range(0,n): \n",
    "    num = nums[i]\n",
    "    count = 1\n",
    "    while num + 1 in nums: \n",
    "        count += 1\n",
    "        num = num + 1\n",
    "    max_count = max(max_count,count)\n",
    "print(max_count)"
