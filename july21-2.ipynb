    "# Enter Odd values and the second value must be 3 times of the first \n",
    "N, M = map(int, input().split())\n",
    "for i in range(1, N, 2):\n",
    "    print(('.|.'* i).center(M,'-'))    \n",
    "print('WELCOME'.center(M,'-'))\n",
    "for i in range(N-1, -1, -2):\n",
    "    print(('.|.'* i).center(M,'-'))"
