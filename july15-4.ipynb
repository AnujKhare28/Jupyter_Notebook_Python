   "source": [
    "import random, itertools\n",
    "\n",
    "deck = list(itertools.product(range(1,14),['Spade','Club','Heart','Diamond']))\n",
    "\n",
    "random.shuffle(deck)\n",
    "\n",
    "a = int(input('Enter the number of cards you want:'))\n",
    "for i in range(a):\n",
    "    print(deck[i][0],'of',deck[i][1])"
   ]
